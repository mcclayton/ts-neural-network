{
  "name": "ts-neural-network",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "node scripts/prepare.js",
    "start": "vite",
    "build": "tsc && vite build",
    "lint:fix": "npm run lint:check -- --fix",
    "lint:check": "eslint --ext .tsx,.ts ./src -c .eslintrc.js",
    "format:fix": "prettier --write \"src/**/*.(ts|tsx)\"",
    "format:check": "prettier --check \"src/**/*.(ts|tsx)\"",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.6.8",
    "@vanilla-extract/css-utils": "^0.1.3",
    "@vanilla-extract/dynamic": "^2.0.3",
    "@vanilla-extract/recipes": "^0.5.0",
    "@vanilla-extract/vite-plugin": "^3.9.3",
    "classnames": "^2.2.6",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "^3.2.0",
    "@types/eslint": "^8.44.3",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-local-rules": "^2.0.0",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "3.1.1",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.2.2"
  },
  "lint-staged": {
    "*.{tsx,ts}": [
      "prettier --write",
      "eslint --cache --fix --report-unused-disable-directives -c .eslintrc.strict.js"
    ],
    "*.svg": [
      "prettier --write --parser xml"
    ]
  }
}
